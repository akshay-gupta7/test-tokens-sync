{
  "card": {
    "padding": {
      "value": "{dimension.md}",
      "type": "dimension"
    },
    "background": {
      "value": "{bg.default}",
      "type": "color"
    },
    "borderRadius": {
      "value": "{borderRadius.lg}",
      "type": "borderRadius"
    }
  },
  "button": {
    "primary": {
      "text": {
        "value": "{accent.onAccent}",
        "type": "color"
      },
      "background": {
        "value": "{accent.default}",
        "type": "color"
      }
    },
    "borderWidth": {
      "value": "{dimension.sm}",
      "type": "borderWidth"
    },
    "borderRadius": {
      "value": "{borderRadius.lg}",
      "type": "borderRadius"
    }
  },
  "boxShadow": {
    "default": {
      "value": [
        {
          "x": "5",
          "y": "5",
          "blur": "5",
          "type": "dropShadow",
          "color": "rgba({shadows.default}, 0.15)",
          "spread": "3"
        },
        {
          "x": "4",
          "y": "4",
          "blur": "5",
          "type": "innerShadow",
          "color": "#00000033",
          "spread": "6"
        }
      ],
      "type": "boxShadow"
    }
  },
  "typography": {
    "H1": {
      "Bold": {
        "value": {
          "fontSize": "{fontSizes.h1}",
          "fontFamily": "{fontFamilies.heading}",
          "fontWeight": "{fontWeights.headingBold}",
          "lineHeight": "{lineHeights.heading}",
          "letterSpacing": "{letterSpacing.decreased}",
          "paragraphSpacing": "{paragraphSpacing.h1}"
        },
        "type": "typography"
      },
      "Regular": {
        "value": {
          "fontSize": "{fontSizes.h1}",
          "fontFamily": "{fontFamilies.heading}",
          "fontWeight": "{fontWeights.headingRegular}",
          "lineHeight": "{lineHeights.heading}",
          "letterSpacing": "{letterSpacing.decreased}",
          "paragraphSpacing": "{paragraphSpacing.h1}"
        },
        "type": "typography"
      }
    },
    "H2": {
      "Bold": {
        "value": {
          "fontSize": "{fontSizes.h2}",
          "fontFamily": "{fontFamilies.heading}",
          "fontWeight": "{fontWeights.headingBold}",
          "lineHeight": "{lineHeights.heading}",
          "letterSpacing": "{letterSpacing.decreased}",
          "paragraphSpacing": "{paragraphSpacing.h2}"
        },
        "type": "typography"
      },
      "Regular": {
        "value": {
          "fontSize": "{fontSizes.h2}",
          "fontFamily": "{fontFamilies.heading}",
          "fontWeight": "{fontWeights.headingRegular}",
          "lineHeight": "{lineHeights.heading}",
          "letterSpacing": "{letterSpacing.decreased}",
          "paragraphSpacing": "{paragraphSpacing.h2}"
        },
        "type": "typography"
      }
    },
    "Body": {
      "value": {
        "fontSize": "{fontSizes.body}",
        "fontFamily": "{fontFamilies.body}",
        "fontWeight": "{fontWeights.bodyRegular}",
        "lineHeight": "{lineHeights.heading}",
        "paragraphSpacing": "{paragraphSpacing.h2}"
      },
      "type": "typography"
    }
  }
}