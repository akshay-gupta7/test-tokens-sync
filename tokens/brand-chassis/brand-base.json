{
  "figma": {
    "switch": {
      "brand": {
        "mode-1": {
          "$type": "boolean",
          "$value": "true"
        }
      }
    }
  },
  "color": {
    "base": {
      "palette": {
        "light": {
          "neutral": {
            "base": {
              "$type": "color",
              "$value": "#6F8085"
            }
          },
          "danger": {
            "base": {
              "$type": "color",
              "$value": "#F52314"
            }
          },
          "success": {
            "base": {
              "$type": "color",
              "$value": "#10C263"
            }
          },
          "warning": {
            "base": {
              "$type": "color",
              "$value": "#F5A300"
            },
            "contrast": {
              "$type": "color",
              "$value": "{color.base.palette.light.black.base}"
            }
          },
          "info": {
            "base": {
              "$type": "color",
              "$value": "#1414F5"
            }
          },
          "brand": {
            "base": {
              "$type": "color",
              "$value": "#00A4CC"
            }
          },
          "accent": {
            "base": {
              "$type": "color",
              "$value": "#F55200"
            }
          }
        }
      }
    }
  },
  "typography": {
    "fontFamily": {
      "text": {
        "$type": "fontFamilies",
        "$value": "Inter, system-ui, -apple-system, Helvetica, Arial, sans-serif"
      },
      "display": {
        "$type": "fontFamilies",
        "$value": "'Archivo Narrow', Georgia, 'Times New Roman', Times, serif"
      },
      "code": {
        "$type": "fontFamilies",
        "$value": "Fira Code, Menlo, Monaco, Consolas, 'Courier New', monospace"
      },
      "icon": {
        "$type": "fontFamilies",
        "$value": "Chassis-Icons"
      }
    },
    "fontWeight": {
      "text": {
        "normal": {
          "$type": "fontWeights",
          "$value": "Regular"
        },
        "strong": {
          "$type": "fontWeights",
          "$value": "Semi Bold"
        },
        "mass": {
          "$type": "fontWeights",
          "$value": "Bold"
        },
        "elegant": {
          "$type": "fontWeights",
          "$value": "Light"
        }
      },
      "display": {
        "normal": {
          "$type": "fontWeights",
          "$value": "Medium"
        },
        "strong": {
          "$type": "fontWeights",
          "$value": "SemiBold"
        },
        "mass": {
          "$type": "fontWeights",
          "$value": "Bold"
        },
        "elegant": {
          "$type": "fontWeights",
          "$value": "Regular"
        }
      },
      "code": {
        "normal": {
          "$type": "fontWeights",
          "$value": "Regular"
        },
        "strong": {
          "$type": "fontWeights",
          "$value": "Bold"
        }
      }
    }
  },
  "borderRadius": {
    "base": {
      "xsmall": {
        "$type": "borderRadius",
        "$value": "{dimension.base.2}"
      },
      "small": {
        "$type": "borderRadius",
        "$value": "{dimension.base.4}"
      },
      "medium": {
        "$type": "borderRadius",
        "$value": "{dimension.base.6}"
      },
      "large": {
        "$type": "borderRadius",
        "$value": "{dimension.base.10}"
      },
      "xlarge": {
        "$type": "borderRadius",
        "$value": "{dimension.base.12}"
      },
      "2xlarge": {
        "$type": "borderRadius",
        "$value": "{dimension.base.16}"
      },
      "3xlarge": {
        "$type": "borderRadius",
        "$value": "{dimension.base.20}"
      }
    }
  }
}